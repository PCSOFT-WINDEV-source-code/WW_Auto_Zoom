     code : |1-
      // Summary: Pre-loads the images
      // Syntax:
      // Preload (<sAlias> is string, <sThumbnailImage> is string, <sImageRealSize> is string)
      //
      // Parameters:
      //	sAlias (string): Alias of the image control
      //	sThumbnailImage (string): Path of the thumbnail image
      //	sImageRealSize (string): Path of the image in real size
      // Return value:
      // 	None
      //
      PROCEDURE Preload(LOCAL sAlias is string, LOCAL sImageThumbnail is string, LOCAL sImageRealSize is string)
      
      pclControl 	is object dynamic
      pclThumb	is object dynamic
      
      // Retrieves the corresponding HTML object
      pclControl = GetPageObject(sAlias)
      
      IF pclControl <> Null THEN
      	
      	// No image in real size?
      	IF sImageRealSize = "" THEN
      		
      		// Display the thumbnail image
      		sImageRealSize = sImageThumbnail
      	END
      	
      	// Store the path of the image in  the tag itself
      	pclControl:setAttribute("thumb", (sImageRealSize))		
      	pclControl:style:cursor = "url(" + FolderWeb() + "cursor/zoomin.cur), pointer"
      	
      	// Retrieves the image in real size
      	pclThumb = GetPageObject("thb" + sAlias)
      	pclThumb:src = sImageRealSize
      END
      
      HideImage()
