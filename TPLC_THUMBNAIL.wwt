      // Summary: Hides the image in real size
      // Syntax:
      // New_Procedure ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE HideImage()
      
      pclBox is object dynamic
      
      // Stops the animation if it is in progress
      IF gnAnimationTimer <> 0 THEN
      	EndTimer(gnAnimationTimer)
      	gnAnimationTimer = 0
      END
      
      // Retrieves the box
      pclBox = GetPageObject("box" + __IMAGEALIAS)
      
      SetOpacity(pclBox, 0)
      pclBox:style:visibility = "hidden"
      pclBox:style:left		= "-9999px"
      pclBox:style:top		= "0px"
      pclBox:style:zIndex 	= "0"
