      // Summary: Animates the appearance of the image in real size
      // Syntax:
      // New_Procedure ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE Animation()
      
      pclBox 		is object dynamic
      nOpacity 	is int
      
      // Retrieves the box of the image in real size
      pclBox   = GetPageObject("box" + __IMAGEALIAS)
      
      nOpacity = Val(pclBox:getAttribute("opacity"))
      nOpacity += 10	
      
      IF nOpacity > 100 THEN
      	
      	// End of timer
      	EndTimer(gnAnimationTimer)
      	gnAnimationTimer = 0
      	RETURN
      END
      
      SetOpacity(pclBox, nOpacity)	
