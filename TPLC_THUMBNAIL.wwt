           code : |1+
            
            
            EXTERN window
            
            pclBox		is object dynamic
            pclThumb 	is object dynamic
            pclControl 	is object dynamic
            nWidth	is int
            nHeight	is int
            rRatioW		is real = 1.0
            rRatioH		is real = 1.0
            rRatio 		is real = 1.0
            nPosX		is int
            nPosY		is int
            
            // Retrieves the image in real size
            pclThumb = GetPageObject("thb" + __IMAGEALIAS)
            pclBox   = GetPageObject("box" + __IMAGEALIAS)
            pclControl = GetPageObject(__IMAGEALIAS)
            
            // If the initial width was not stored
            IF pclThumb:getAttribute("orgwidth") THEN
            	
            	// Retrieves the initial stored width
            	nWidth = pclThumb:getAttribute("orgwidth") 		
            ELSE
            	
            	// Retrieves the initial width
            	nWidth = pclThumb:offsetWidth
            	
            	// Saves the initial width in an attribute on the object
            	pclThumb:setAttribute("orgwidth", nWidth)
            END	
            
            // If the initial height was not stored
            IF pclThumb:getAttribute("orgheight") THEN
            	
            	// Retrieves the initial stored height
            	nHeight = pclThumb:getAttribute("orgheight") 	
            ELSE
            	
            	// Retrieves the initial height
            	nHeight = pclThumb:offsetHeight
            	
            	// Saves the initial height in an attribute on the object
            	pclThumb:setAttribute("orgheight", nHeight)
            END
            
            // Ratio of the image
            IF nWidth > nHeight THEN
            	rRatio = nHeight / nWidth 
            ELSE 
            	rRatio = nWidth / nHeight 
            END
            
            // Ratio for reduction in width
            IF nWidth > __MAXWIDTH THEN	
            	rRatioW = __MAXWIDTH / nWidth	
            	nWidth = __MAXWIDTH
            	nHeight = nHeight * rRatioW
            END
            
            // Ratio for reduction in height
            IF nHeight > __MAXHEIGHT THEN	
            	rRatioH = __MAXHEIGHT / nHeight	
            	nHeight = __MAXHEIGHT
            	nWidth = nWidth * rRatioH
            END
            
            // Calculates the position of the image
            nPosX = (MySelf..Width - nWidth) / 2 + MySelf..X
            nPosY = (MySelf..Height - nHeight) / 2 + MySelf..Y
            
            // Restores the position to 0 if it is less
            IF nPosX < 0 THEN nPosX = 0
            IF nPosY < 0 THEN nPosY = 0
            IF nPosX + nWidth > window:document:body:offsetWidth THEN nPosX = window:document:body:offsetWidth - nWidth - 64
            IF nPosY + nHeight > window:document:body:offsetHeight THEN nPosY = window:document:body:offsetHeight - nHeight - 64
            
            pclBox:removeAttribute("opacity")
            
            // Resizes the image
            pclThumb:style:width 		= nWidth
            pclThumb:style:height 		= nHeight
            
            // Displays the image
            pclBox:style:left			= nPosX + "px"
            pclBox:style:top			= nPosY + "px"
            pclBox:style:zIndex 		= MaxZIndex()
            pclBox:style:visibility 	= "visible"
            pclThumb:style:visibility 	= "visible"
            
            // Starts the animation
            gnAnimationTimer = Timer(Animation, 2)
