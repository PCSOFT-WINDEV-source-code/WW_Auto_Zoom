      // Summary: Initializes the control template
      // Syntax:
      // InitThumbnail ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE InitThumbnail()
      
      // Retrieves the alias of the control template
      __ALIAS = MySelf..Alias
      
      // Retrieves the alias of the image control
      __IMAGEALIAS = IMG_TPLIMAGE..Alias
      
      IMG_TPLIMAGE..HTMLBefore = StringBuild([
      	<div id='box%1' name='box%1' style='padding:4px; position:absolute; top: 0px; left: -9999px; visibility: hidden; color:#fff; background: white;'>
      	<img id='thb%1' name='thb%1' vspace=4 hspace=4 border=0 onclick='%4_HIDEIMAGE();' style='cursor: url(/%5/cursor/zoomout.cur), pointer'>
      	</div>
      	<div style='width: %2px; height: %3px'>
      ], __IMAGEALIAS, IMG_TPLIMAGE..Width, IMG_TPLIMAGE..Height, __ALIAS, FolderWeb)
      
      IMG_TPLIMAGE..HTMLAfter = "</div>"
