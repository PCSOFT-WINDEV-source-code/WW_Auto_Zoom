     code : |1-
      // Summary: <specify the action of the procedure>
      // Syntax:
      //[ <Result> = ] MaxZIndex ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	8-byte integer: // 	None
      //
      // Example:
      // Indicate an example.
      //
      PROCEDURE MaxZIndex()
      
      EXTERN document
      
      nIndex is int = 0
      arrList is array of 0 object dynamic
      
      IF document:all THEN
      	
      	// Retrieves all the objects of the page
      	arrList = document:all
      ELSE
      	
      	// Retrieves all the objects of the page
      	arrList = document:getElementsByTagName("div")
      END
      
      FOR i = 1 _TO_ Dimension(arrList)
      	// Retrieve the zIndex if it is greater
      	pclDiv is dynamic object = arrList[i]
      	if pclDiv <> null _and_ pclDiv:style then
      		IF nIndex < Val(arrList[i]:style:zIndex) THEN				
      			nIndex = Val(arrList[i]:style:zIndex)
      		END
      	end
      END
      
      RESULT (nIndex + 1)
